				{
					"name": "org_user_node_acc",
					"mapping-field": "node_id",
					"mapping-field-type": "Long",
					"lookup-field": "node_name"
				}
			]
		},
		"position": {
			"field": "node_id",
			"lookup": [
				{
					"name": "org_user_node_acc",
					"mapping-field": "node_id",
					"mapping-field-type": "Long",
					"lookup-field": "position"
				}
			]
		},
		"user": {
			"field": "user_id",
			"lookup": [
				{
					"name": "org_user_node_acc",
					"mapping-field": "user_id",
					"lookup-field": "user_name"
				}
			]
		},
		"course_name": {
			"field": "course_name"
		},
		"course_type": {
			"field": "course_type",
